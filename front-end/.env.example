# Front-end (Expo) environment variables
# Copy this file to .env and fill in your actual values
# Only EXPO_PUBLIC_* keys are kept here. Do NOT put secrets (DB, API secret, webhook secret).

# Backend API URL (Development: use ngrok URL, Production: use your deployed URL)
EXPO_PUBLIC_IP=https://your-backend-url.com

# Translation API URL (Development: use ngrok URL, Production: use your deployed URL)
EXPO_PUBLIC_TRANSLATE_API_BACKEND_URL=https://your-translate-api-url.com

# Clerk Publishable Key (Get from Clerk Dashboard)
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_key

# Gemini API Key (Get from Google AI Studio)
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key
